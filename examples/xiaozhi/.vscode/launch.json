{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: App",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "justMyCode": false,
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Python: Current",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "justMyCode": false,
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
